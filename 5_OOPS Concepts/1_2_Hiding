using System;
class Learn
{
    public void display()
    {
        Console.WriteLine("hello this is Learn class");
    }
}
class Prog:Learn
{
    public new void display()
    {
        Console.WriteLine("helloo this is prog class");
    }
    
}
class HelloWorld {
  static void Main() {
Learn obj = new Prog();
obj.display();
  }
}
